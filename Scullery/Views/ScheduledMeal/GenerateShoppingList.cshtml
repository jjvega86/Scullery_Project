@model Scullery.Models.ViewModels.ShoppingList
@{
    ViewData["Title"] = "GenerateShoppingList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1 class="text-primary">Shopping List</h1>
<br />
<table class="table">
    <thead>
        <tr>

            <th>
                Item Name
            </th>            
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Items)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.name)
            </td>
            
        </tr>
        }
    </tbody>
</table>